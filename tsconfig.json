{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"declaration": true,
		"outDir": "dist",
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noUnusedLocals": true
	},
	"include": ["index.ts"]
}
